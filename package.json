{
  "name": "node-graphql-starter",
  "version": "0.0.1",
  "description": "A sample for a fully node server setup using GraphQL, Typescript, Docker and PostgresSQL",
  "repository": "",
  "scripts": {
    "build": "rm -rf dist || true && tsc",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "test": "TEST=true mocha -r ts-node/register --timeout 10000 --exit test/index.ts"
  },
  "dependencies": {
    "dotenv": "8.0.0",
    "graphql-yoga": "1.18.1",
    "pg": "7.12.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.18"
  },
  "devDependencies": {
    "@types/chai": "4.2.0",
    "@types/mocha": "5.2.7",
    "@types/node": "10.14.13",
    "@types/supertest": "2.0.8",
    "chai": "4.2.0",
    "mocha": "6.2.0",
    "nodemon": "1.19.1",
    "supertest": "4.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "license": "MIT"
}
